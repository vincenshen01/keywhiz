<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- Skip jOOQ generated code. -->
  <Match>
    <Package name="keywhiz.jooq"/>
  </Match>
  <Match>
    <Package name="keywhiz.jooq.tables"/>
  </Match>

  <!-- Don't complain about mockito in tests. -->
  <Match>
    <Field name="~.*mockito"/>
    <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD" />
  </Match>

</FindBugsFilter>
